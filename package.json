{
  "name": "babel-rewire-wrapper",
  "version": "1.0.0",
  "description": "Wrapper of babel-plugin-rewire.",
  "homepage": "https://github.com/ryym/babel-rewire-wrapper",
  "repository": {
    "type": "git",
    "url": "https://github.com/ryym/babel-rewire-wrapper"
  },
